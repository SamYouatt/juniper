{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-use-before-define": "off",
        "react/style-prop-object": "warn",
        "no-unused-vars": "warn",
        "react/prop-types": "off",
        "global-require": "warn",
        "quote-props": "off",
        "max-len": "warn",
        "no-plusplus": "off",
        "no-param-reassign": "off",
        "import/no-dynamic-require": "error",
        "dot-notation": "warn"
    }
}
